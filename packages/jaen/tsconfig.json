{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "@actions/*": ["store/actions/*"],
      "@store/*": ["store/*"],
      "@src/*": ["./*"],
      "@reducers/*": ["store/reducers/*"],
      "@containers/*": ["containers/*"]
    }
  },
  "include": ["src", "src/gatsby"]
}
