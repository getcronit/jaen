{
  "presets": [
    "babel-preset-gatsby-package",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-relative-path-import",
      {
        "paths": [
          {
            "rootPathPrefix": "@src",
            "rootPathSuffix": "src"
          },
          {
            "rootPathPrefix": "@store",
            "rootPathSuffix": "src/store"
          },
          {
            "rootPathPrefix": "@actions",
            "rootPathSuffix": "src/store/actions"
          },
          {
            "rootPathPrefix": "@reducers",
            "rootPathSuffix": "src/store/reducers"
          },
          {
            "rootPathPrefix": "@common",
            "rootPathSuffix": "src/common"
          },
          {
            "rootPathPrefix": "@containers",
            "rootPathSuffix": "src/containers"
          },
          {
            "rootPathPrefix": "@contexts",
            "rootPathSuffix": "src/contexts"
          }
        ]
      }
    ]
  ]
}
