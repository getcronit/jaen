{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "@actions/*": ["store/actions/*"],
      "@store/*": ["store/*"],
      "@src/*": ["./*"],
      "@reducers/*": ["store/reducers/*"],
      "@containers/*": ["containers/*"],
      "@contexts/*": ["contexts/*"],
      "@common/*": ["common/*"]
    }
  },
  "include": ["src", "gatsby-browser.js", "register.tsx"],
  "typeRoots": ["./@types"],
  "references": [{"path": "../jaen"}, {"path": "../jaen-shared-ui"}]
}
